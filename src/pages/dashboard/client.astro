---
/**
 * Client/Owner Dashboard Page
 * High-level oversight dashboard for project owners
 */
import Layout from '../../layouts/Layout.astro';

export const prerender = false;
---

<Layout title="Owner Dashboard - ConstructAid">
  <!-- Client Dashboard Container -->
  <div id="client-dashboard"></div>

  <script>
    import { render } from 'solid-js/web';
    import ClientDashboard from '../../components/ClientDashboard';

    const container = document.getElementById('client-dashboard');
    if (container) {
      render(() => ClientDashboard({}), container);
    }
  </script>
</Layout>
